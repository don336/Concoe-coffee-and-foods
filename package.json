  "bugs": {
    "url": "https://github.com/don336/Concoe-coffee-and-foods/issues"
  },
  "homepage": "https://github.com/don336/Concoe-coffee-and-foods#readme",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "celebrate": "^15.0.1",
    "cors": "^2.8.5",
    "env-cmd": "^10.1.0",
    "express": "^4.18.2",
    "husky": "^8.0.3",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.2",
    "nodemon": "^2.0.21",
    "uuid": "^9.0.0",
    "uuidv4": "^6.2.13"
  },
